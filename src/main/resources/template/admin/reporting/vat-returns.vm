#parse("/template/admin/layout.vm")
#@mainLayout()

<!-- Content Header (Page header) -->
<section class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-sm-6">
                <h1 class="m-0 text-dark">VAT Returns</h1>
            </div>
        </div><!-- /.row -->
    </div><!-- /.container-fluid -->
</section>
<!-- /.content-header -->

<!-- Main content -->
<section class="content">
    <!--<div class="container-fluid">-->
    <!-- Main row -->
    <div class="row">
        <div class="col-12">
            <div class="card card-primary card-outline card-outline-tabs">
                <div class="card-header p-0 border-bottom-0">
                    <ul class="nav nav-tabs" id="custom-tabs-four-tab" role="tablist">
                        <li class="nav-item">
                            <a class="nav-link active" id="custom-tabs-four-monthly-tab" data-toggle="pill" href="#custom-tabs-four-monthly" role="tab" aria-controls="custom-tabs-four-monthly" aria-selected="true">Monthly</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" id="custom-tabs-four-trimester-tab" data-toggle="pill" href="#custom-tabs-four-trimester" role="tab" aria-controls="custom-tabs-four-trimester" aria-selected="false">Trimester</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" id="custom-tabs-four-semester-tab" data-toggle="pill" href="#custom-tabs-four-semester" role="tab" aria-controls="custom-tabs-four-semester" aria-selected="false">Semester</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" id="custom-tabs-four-annual-tab" data-toggle="pill" href="#custom-tabs-four-annual" role="tab" aria-controls="custom-tabs-four-annual" aria-selected="false">Annual</a>
                        </li>
                    </ul>
                </div>
                <div class="card-body">
                    <div class="tab-content" id="custom-tabs-four-tabContent">
                        <div class="tab-pane fade active show" id="custom-tabs-four-monthly" role="tabpanel" aria-labelledby="custom-tabs-four-monthly-tab">
                            List of Monthly government files</div>
                        <div class="tab-pane fade" id="custom-tabs-four-trimester" role="tabpanel" aria-labelledby="custom-tabs-four-trimester-tab">
                            List of Trimester government files
                        </div>
                        <div class="tab-pane fade" id="custom-tabs-four-semester" role="tabpanel" aria-labelledby="custom-tabs-four-semester-tab">
                            List of Semester government files
                        </div>
                        <div class="tab-pane fade" id="custom-tabs-four-annual" role="tabpanel" aria-labelledby="custom-tabs-four-annual-tab">
                            List of Annual government files
                        </div>
                    </div>
                </div>
                <!-- /.card -->
            </div>
        </div>
    </div> <!-- /Main row -->
    <!--</div> /.container-fluid -->
</section>


#end
